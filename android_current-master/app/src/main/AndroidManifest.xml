<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.RobertsKunicins.My_App">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_cat"
        android:label="My App"
        android:roundIcon="@mipmap/ic_cat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.RobertsKunicins.My_App.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.RobertsKunicins.My_App.fragments.MainFragment"></activity>
    </application>

</manifest>